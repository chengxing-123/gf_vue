   var pmsg=this.msg;\r\n             //判断新闻评论内容不能为空\r\n             if(pmsg.trim().length==0){\r\n               Toast(\"评论内容不能为空!\")  \r\n               return;\r\n             }\r\n             //发送post请求\r\n             var url=\"http://127.0.0.1:3000/postcomment\";\r\n             this.$http.post(url,{nid:nid,msg:pmsg}).then(result=>{\r\n                console.log(result)\r\n                Toast(\"发表成功\");\r\n                this.pageIndex=0;\r\n                this.list=[];\r\n                this.getMore();\r\n             })\r\n             //获取服务器程序返回结果\r\n             //提示用户评论成功\r\n\r\n             \r\n         }\r\n     },\r\n     created(){\r\n         this.getMore()\r\n     }\r\n    \r\n }\r\n\r\n</script>\r\n<style>\r\n.app-comment h1{\r\n    font-size:18px;\r\n    \r\n}\r\n.app-comment textarea{\r\n    font-size:14px;\r\n    height:85px;\r\n    margin:0;\r\n}\r\n.app-comment .cmt-list{\r\n    margin:5px 0;\r\n\r\n}\r\n.cmt-list .cmt-item .cmt-info{\r\n    line-height:30px;\r\n    background:#ccc;\r\n\r\n}\r\n.cmt-list .cmt-item .cmt-body{\r\n    line-height:35px;\r\n    text-indent:2em;\r\n}\r\n</style>"],"sourceRoot":"src\\components\\sub"}]}