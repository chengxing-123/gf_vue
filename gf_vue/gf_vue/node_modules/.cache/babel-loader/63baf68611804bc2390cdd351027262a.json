   <mt-button type=\"danger\" size=\"small\" @click=\"addCartTo\">加入购物车</mt-button>\r\n           </p>\r\n        </div>\r\n    </div>\r\n    <div class=\"mui-card-footer\"></div>\r\n  </div>\r\n <!--3商品参数区域-->\r\n   <div class=\"mui-card\">\r\n    <div class=\"mui-card-header\">商品参数</div>\r\n    <div class=\"mui-card-content\">\r\n        <div class=\"mui-card-content-inner\">\r\n            <p>商品货号：{{info.pid}}</p>\r\n            <p>商品颜色：山里红</p>\r\n            <p>商品运费：0</p>\r\n        </div>\r\n    </div>\r\n    <div class=\"mui-card-footer\">页脚</div>\r\n  </div>\r\n </div>\r\n</template>\r\n<script>\r\n//引入轮播图组件\r\nimport swiper from \"../sub/swiper.vue\";\r\n//引入js库\r\n//import mui from \"../../lib/mui/js/mui.js\";\r\nimport {Toast} from \"mint-ui\";\r\nexport default {\r\n    data(){\r\n        return{\r\n            imglist:[],\r\n            val:1,\r\n            info:{}\r\n\r\n        }\r\n    },\r\n    created(){\r\n        console.log(this.$route.params.id);\r\n         this. getImageList();\r\n         this.getGoodInfo()\r\n       \r\n    },\r\n    methods:{\r\n       addCartTo(){\r\n        //console.log(\"添加至购物车\");\r\n       // 1.将商品编号和数量保存服务器\r\n\r\n            // 1.1 获取商品编号\r\n            // 1.2 获取商品数量\r\n            //1.3 发送请求\r\n        var id=this.$route.params.id;\r\n        var count=this.val\r\n       this.$http.get(\"addCart?pid=\"+id+\"&count=\"+count).then(result=>{\r\n           if(result.body.code==1){\r\n               //更新购物车中商品数量\r\n               //修改 Vuex中共享数据\r\n               this.$store.commit(\"increment\",count);\r\n\r\n               Toast(result.body.msg);\r\n           }else{\r\n               Toast(result.body.msg);\r\n           }\r\n       })\r\n        //2.更新APP.vue 组件购物车数量\r\n\r\n       },\r\n        //服务器数据，返回模拟数据{id,title,now,old,商号}\r\n        //参数：商品id\r\n        getGoodInfo(){\r\n            var id=this.$route.params.id;\r\n            this.$http.get('goodsinfo?id='+id).then(result=>{\r\n             \r\n               this.info=result.body;\r\n               // console.log(this.info);\r\n            })\r\n        },\r\n       getImageList(){\r\n          \r\n          this.$http.get(\"imagelist\").then(result=>{\r\n             console.log(result);\r\n             this.imglist=result.body;\r\n          })\r\n    },\r\n\r\n    goodAdd(){\r\n        if(this.val<=998)\r\n        {\r\n         this.val++;\r\n         }\r\n         \r\n         },\r\n\r\n    \r\n    goodSub(){\r\n      if(this.val>1)\r\n      {\r\n        this.val--  ;\r\n      }\r\n     \r\n    }\r\n    },\r\n    components:{\r\n        \"swiper-box\":swiper\r\n    }\r\n}\r\n</script>\r\n<style>\r\n\r\n</style>"],"sourceRoot":"src\\components\\goods"}]}